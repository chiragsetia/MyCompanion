<% include ..\partials\header %>
<% include ..\partials\forManager %>
<div class="row">
	<% leaves.forEach(function(leave){ %>
		<div class="col-lg-3 col-md-3" ><%= leave.author.rollNumber%></div>
		<div class="col-lg-3 col-md-3" ><%= leave.leaveDate %></div>
		<div class="col-lg-3 col-md-3" ><%= leave.returnDate %></div>
		<div class="col-lg-3 col-md-3" >
			<form action="/Manager/CLE/<%=leave._id%>?_method=DELETE" method="POST">
				<input type="submit" class="btn btn-md btn-danger" value="Delete">
			</form>
		</div>
	<% }) %>
</div>

<div class="container">
	<a href="/Manager" class="btn btn-primary btn-lg btn-block">
	BACK
	</a>
</div>

<% include ..\partials\footer %>